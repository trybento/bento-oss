{
  "name": "shoyu",
  "author": "Bento",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "concurrently --names 'ENTRY,GQL,BUILD,SERVER' --prefix-colors 'yellow,magenta,red,green' \"vite build -c vite.entry-point.config.js --mode development\" \"yarn generate:gql-schemas --watch\" \"NODE_ENV=development VITE_COMMIT_SHA=dev vite build --watch --mode development\" \"http-server ./build -p 8082 -b --cors -c-1\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --fix . --ext .js,.jsx,.ts,.tsx --quiet && prettier --write .",
    "checks": "eslint . --ext .js,.jsx,.ts,.tsx --quiet && prettier --check .",
    "graphql-checks": "yarn validate-queries",
    "validate-queries": "graphql-inspector validate --noStrictFragments ./src/graphql ../udon/src/graphql/__generated__/embed.graphql",
    "generate:gql-schemas": "graphql-codegen --config codegen.yml"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
